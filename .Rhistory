make_compare_table <- function(df, var, covars, group = "Change_Type") {
if (!is.numeric(df[[var]])) return(NULL)
# Model cho effect size & marginal means
model <- lm(as.formula(paste(var, "~", group, "+", paste(covars, collapse = "+"))), data = df)
emm <- suppressMessages(emmeans(model, specs = group))
pair <- pairs(emm, adjust = "tukey")
pair_df <- as.data.frame(pair)
eff <- NA
try({ eff <- effectsize::hedges_g(model, var = group)[1,"Hedges_g"] }, silent = TRUE)
means <- df %>%
group_by(.data[[group]]) %>%
summarise(
N = sum(!is.na(.data[[var]])),
Mean = mean(.data[[var]], na.rm = TRUE),
SD = sd(.data[[var]], na.rm = TRUE)
) %>%
arrange(match(.data[[group]], levels(df[[group]])))
# Lấy dòng p-value so sánh ProgressiveCN - StableCN
out <- tibble::tibble(
Variable = var,
Mean_StableCN = means$Mean[1],
Mean_ProgressiveCN = means$Mean[2],
SD_StableCN = means$SD[1],
SD_ProgressiveCN = means$SD[2],
Mean_Diff = pair_df$estimate[1],
CI_lower = pair_df$lower.CL[1],
CI_upper = pair_df$upper.CL[1],
p.value = pair_df$p.value[1],
Hedges_g = eff
)
out
}
